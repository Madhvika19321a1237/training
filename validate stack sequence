
        class Solution {
public:
    bool validateStackSequences(vector<int>& pushed, vector<int>& popped) {
        stack<int> intStack;
        int poppedIndex = 0;
        
        for (int i = 0; i < pushed.size(); i++)
        {
            intStack.push(pushed[i]);
            
            while (!intStack.empty() && popped[poppedIndex] == intStack.top())
            {
                intStack.pop();
                poppedIndex++;
            }
        }
        
        return poppedIndex == popped.size();
    }
};
